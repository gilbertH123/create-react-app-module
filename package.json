{
  "name": "create-react-component",
  "version": "1.0.0",
  "description": "Create React components with no build configuration.",
  "main": "tools/server.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint template scripts",
    "lint:watch": "npm run lint -- --watch",
    "compile": "./node_modules/.bin/babel --out-dir bin scripts",
    "prepublish": "npm run lint & npm run compile"
  },
  "bin": {
    "component-builder": "./bin/init.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snehatulsi/create-react-component.git"
  },
  "keywords": [
    "component",
    "module",
    "starter kit"
  ],
  "author": "Sneha Jain <mystery.sneha@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "cross-spawn": "^4.0.0",
    "fs": "0.0.1-security",
    "minimist": "^1.2.0",
    "node-fs-extra": "^0.8.1",
    "path": "^0.12.7",
    "path-exists": "^3.0.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
